{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.0",
        "laravel/framework": "5.2.*",
        "redcrystal/laravel-validation-rules": "^1.0",
        "league/fractal": "^0.13.0",
        "league/csv": "^8.0",
        "jenssegers/rollbar": "^1.5",
        "ramsey/uuid": "^3.4",
        "doctrine/orm": "^2.5",
        "laravel-doctrine/orm": "1.2.*",
        "laravel-doctrine/extensions": "^1.0",
        "gedmo/doctrine-extensions": "^2.4",
        "vasek-purchart/doctrine-date-time-immutable-types": "^0.3.0",
        "laravel-doctrine/fluent": "^1.1",
        "laravel-doctrine/migrations": "1.0.*",
        "ocramius/generated-hydrator": "^2.0",
        "jdorn/sql-formatter": "^1.2",
        "tymon/jwt-auth": "^0.5.10",
        "lcobucci/jwt": "^3.2"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~4.0",
        "symfony/css-selector": "2.8.*|3.0.*",
        "symfony/dom-crawler": "2.8.*|3.0.*",
        "barryvdh/laravel-ide-helper": "^2.1",
        "doctrine/dbal": "^2.5"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "Hourglass\\": "app/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
